<IfModule mod_proxy.c>

  ProxyRequests Off

  <Proxy *>
    AddDefaultCharset off
    Order deny,allow
    Deny from all
    Allow from all
  </Proxy>
  # reverse proxy galaxy-master
  ProxyPass / http://localhost:8080/
  ProxyPassReverse / http://localhost:8080/

  RewriteEngine on


  ProxyVia On
</IfModule>
